{
    "name": "egeloen/lucene-search-bundle",
    "type": "symfony-bundle",
    "description": "Provides a zend lucene search integration for your Symfony2 Project.",
    "keywords": ["lucene search"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "zendframework/zf2",
                "version": "2.0.0beta3",
                "source": {
                    "type": "git",
                    "url": "http://github.com/zendframework/zf2.git",
                    "reference": "release-2.0.0beta3"
                },
                "autoload": {
                    "psr-0": {
                        "Zend\\": "library/"
                    }
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": "2.*",
        "zendframework/zf2": "2.*"
    },
    "autoload": {
        "psr-0": { "Ivory\\LuceneSearchBundle": "" }
    },
    "target-dir": "Ivory/LuceneSearchBundle"
}