{
    "name": "egeloen/lucene-search-bundle",
    "type": "symfony-bundle",
    "description": "Provides a zend lucene search integration for your Symfony2 Project.",
    "keywords": [ "lucene search" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": "~2.1",
        "symfony/filesystem": "~2.1",
        "zendframework/zendsearch": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "satooshi/php-coveralls": "~0.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zendframework/ZendSearch"
        }
    ],
    "autoload": {
        "psr-0": { "Ivory\\LuceneSearchBundle": "" }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "target-dir": "Ivory/LuceneSearchBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}